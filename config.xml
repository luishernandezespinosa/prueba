<?xml version='1.0' encoding='utf-8'?>
<widget id="es.agendacita.demo" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>agendacita.es demo empresas</name>
    <description>
        agendacita.es reserva de cita previa para empresas
    </description>
    <author email="hola@agendacita.es" href="https://agendacita.es">
        agendacita.es
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />

    <splash src="splash.png" />
    <preference name="SplashScreenDelay" value="8000" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="ShowSplashScreenSpinner" value="true"/>
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />

    
  
    <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~3.0.0" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0" />
    <plugin name="cordova-plugin-splashscreen" source="npm" spec="~3.2.1" />

    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    
   <preference name="InAppBrowserOverrideUserAgent" value="Mozilla/5.0 Google" />
   <preference name="OverrideUserAgent" value="Mozilla/5.0 Google" />


   <platform name="browser">
    <preference name="SplashScreen" value="splash" /> <!-- defaults to "/img/logo.png" -->
    <preference name="AutoHideSplashScreen" value="true" /> <!-- defaults to "true" -->
    <preference name="SplashScreenDelay" value="8000" /> <!-- defaults to "3000" -->
    
    <preference name="ShowSplashScreen" value="true" /> <!-- defaults to "true" -->

</platform>
    
    <platform name="android">
              <icon src="res/icon/android/drawable-ldpi-icon.png" density="ldpi" />
              <icon src="res/icon/android/drawable-mdpi-icon.png" density="mdpi" />
              <icon src="res/icon/android/drawable-hdpi-icon.png" density="hdpi" />
              <icon src="res/icon/android/drawable-xhdpi-icon.png" density="xhdpi" />

    <splash src="res/screen/android/splash-land-hdpi.png" qualifier="land-hdpi"/>
    <splash src="res/screen/android/splash-land-ldpi.png" qualifier="land-ldpi"/>
    <splash src="res/screen/android/splash-land-mdpi.png" qualifier="land-mdpi"/>
    <splash src="res/screen/android/splash-land-xhdpi.png" qualifier="land-xhdpi"/>

    <splash src="res/screen/android/splash-port-hdpi.png" qualifier="port-hdpi"/>
    <splash src="res/screen/android/splash-port-ldpi.png" qualifier="port-ldpi"/>
    <splash src="res/screen/android/splash-port-mdpi.png" qualifier="port-mdpi"/>
    <splash src="res/screen/android/splash-port-xhdpi.png" qualifier="port-xhdpi"/>

  </platform>

